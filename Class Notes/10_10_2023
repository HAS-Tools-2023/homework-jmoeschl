#%%
# plotting with MATPLOTLIB


# create object named ax... plotting axis and everything layers
# use when using subplots
# explicitly labeling and creating
fig, ax = plt.subplots(figsize=(5, 2.7), layout='constrained')
ax.set_title
ax.set_ylabel
ax.set_xlabel
ax.plot(x, x, label='linear')




# creating behind the scene
plt.figure(figsize=(5, 2.7), layout='constrained')
plt.xlabel('')
plt.title('')
plt.plot # makes a line
plt.plot(x , x**2, label='quadratic')

x = np.linspace(0, 2, 100)      # makes data 0-100 every 2

